{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": ">=14.0"
      }
    ],
    "Package JSON": [
      {
        "rule": "file",
        "location": "./package.json"
      }
    ],
    "NPM": [
      {
        "rule": "cli",
        "binary": "npm",
        "semver": ">=6.0"
      }
    ],
    "Yarn": [
      {
        "rule": "cli",
        "binary": "yarn",
        "version": "--version",
        "semver": ">=1.20.0"
      }
    ]
  }
}

